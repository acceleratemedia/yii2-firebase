{
    "name": "brianvb/yii2-firebase",
    "description": "Extension for talking with Firebase servers using PHP and the Yii 2 Framework",
    "type": "yii2-extension",
    "require": {
        "brianvb/yii2-singleton": "@dev",
        "yiisoft/yii2": "^2.0",
        "kreait/firebase-php": "^4.18"
    },
    "authors": [
        {
            "name": "BrianVB",
            "email": "vanburen.brian@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "bvb\\firebase\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "path",
            "url": "./../yii2-singleton",
            "options": {
                "symlink": true
            }
        }
    ]
}
